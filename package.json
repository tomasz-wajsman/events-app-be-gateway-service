{
  "name": "express-gen-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "NODE_ENV=development ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "start": "NODE_ENV=production node -r ./config.js ./dist",
    "test": "NODE_ENV=test vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.0",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/core": "^1.11.24",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.4",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.26.0",
    "eslint-plugin-n": "^17.18.0",
    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  }
}
